{
  "presets": [
    ["@babel/preset-env", { "targets": { "node": "current" } }],
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": "dev-only",
        "labelFormat": "[local]"
      }
    ]
  ],
  "plugins": [
    "lodash",
    [
      "module-resolver",
      {
        "alias": {
          "@": "./src",
          "@components": "./src/components"
        }
      }
    ]
  ]
}
